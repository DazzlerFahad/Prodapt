package com.fileupload.Entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="file")
public class FileDB {

    @javax.persistence.Id
    //@GeneratedValue(strategy = GenerationType.IDENTITY)
    private String Id;

    private String oltNeId;

    private String olt;

    private String type;

    private String pon;

    private String serialNumber;

    private String zone;

    private String site;

    public String getId() {
        return Id;
    }

    public void setId(String id) {
        Id = id;
    }

    public String getOltNeId() {
        return oltNeId;
    }

    public void setOltNeId(String oltNeId) {
        this.oltNeId = oltNeId;
    }

    public String getOlt() {
        return olt;
    }

    public void setOlt(String olt) {
        this.olt = olt;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getPon() {
        return pon;
    }

    public void setPon(String pon) {
        this.pon = pon;
    }

    public String getSriNo() {
        return sriNo;
    }

    public void setSriNo(String sriNo) {
        this.sriNo = sriNo;
    }

    public String getZone() {
        return zone;
    }

    public void setZone(String zone) {
        this.zone = zone;
    }

    public String getSite() {
        return site;
    }

    public void setSite(String site) {
        this.site = site;
    }

    public FileDB(String id, String oltNeId, String olt, String type, String pon, String sriNo, String zone,
                  String site) {
        super();
        Id = id;
        this.oltNeId = oltNeId;
        this.olt = olt;
        this.type = type;
        this.pon = pon;
        this.sriNo = sriNo;
        this.zone = zone;
        this.site = site;
    }

    public FileDB() {
        super();
        // TODO Auto-generated constructor stub
    }

}
